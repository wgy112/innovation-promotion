<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.sdu.mapper.SduPrincipleMapper">

    <select id="findByCategory" parameterType="int" resultType="org.jeecg.modules.sdu.entity.SduPrinciple">
        SELECT * FROM sdu_principle WHERE CATEGORY = #{category}
    </select>

    <select id="findPrincipleTraditional" resultType="org.jeecg.modules.sdu.entity.SduPrinciple">
        SELECT * FROM sdu_principle WHERE ENHANCEPARA =#{enhancePara} AND AGGRAVATEPARA !=#{aggravatePara}
    </select>

</mapper>
