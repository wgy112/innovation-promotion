<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.sdu.mapper.SduSchemeMapper">
    <select id="findByUser" parameterType="string" resultType= "org.jeecg.modules.sdu.entity.SduScheme">
        SELECT * FROM sdu_scheme WHERE USER = #{user}
    </select>

    <select id="searchNumberById" parameterType="Integer" resultType="int">
        SELECT COUNT(*) FROM sdu_scheme WHERE DESCRIPTIONID = #{id}
    </select>

</mapper>
